datasource db {
    provider = "mongodb"
    url      = env("MONGODB_URI")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id           String   @id @default(auto()) @map("_id") @db.ObjectId
    refreshToken String   @unique @db.String
    email        String   @unique
    picture      String?
    createdAt    DateTime @default(now())
}
